<nav class="navbar navbar-default" role="navigation">
	<div class="navbar-header">
		<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
			<span class="sr-only">Toggle navigation</span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
			<span class="icon-bar"></span>
		</button>
		<a id='links' class="navbar-brand" href="/user_profile">Musicians Meet-Up</a>
	</div>

	<form class="navbar-form navbar-right search" role="search" action='/classifieds' method='GET'>
		<div class="form-group">
			<input type="text" class="form-control" placeholder="Search" name='search'>
		</div>
		<button id='search' type="submit" class="btn btn-default">Submit</button>
	</form>

	<div class="collapse navbar-collapse navbar-ex1-collapse">
		<ul class="nav navbar-nav navbar-right">
			<li><a id='links' href="/user_profile">Me</a></li>
			<li><a id='links' href="/community">Community</a></li>
			<li><a id='links' href="#">Messages</a></li>
			<li><a id='links' href="/logout">Logout</a></li>
		</ul>
	</div>
</nav>
<div class="container">
	<div class='page-header'>
		<h2 class='blue'>Classifieds - <small class='white'> See What Other Musicians Are Looking For</small></h2>
		<a href='/create_post' type="button" class="btn btn-default white">Make a Post</a>
	</div>
			
	
	<% posts.forEach(function(post) { %>
		<div class="well">
			<div class='names'>
				<h4><%=post.title%></h4>
				<p><%=post.body%></p>
				<p><a href='user_profile/<%=post.user.id%>'><%=post.user.firstName + ' ' + post.user.lastName%></a> - <%=post.user.location%></p>
			</div>
			<div class='message'>
				<a id='message' href='/send_message/<%=post.id%>' type="button" class="btn btn-default white">Reply</a>
			</div>
		</div>
	<% }); %>
</div>